#include <stdio.h>
#include <math.h>
int main()
{
    int i,x,val;
    scanf("%d",&x);
    val=ceil(sqrt(x));
    int val2=x, count=0;
    for (i=2; i<val;i++);
    {
        if (val2%i==0)
        count=1; 
    }
    if ((count==0 && val!=1)|| val2==2 || val2==3)
        printf("%d is a prime",val2);
    else
        printf("not a prime number");
}
